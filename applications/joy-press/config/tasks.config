# /bin/tasks rss-reader start|stop|restart
# 
# format
# minute hour day month dayOfWeek
#
# minute: 0-59
# hour: 0-23
# day: 1-31
# month: 1-12
# dayOfWeek: 0-7


rss-reader:
    class:  app.tasks.RssReader
    method: Run
    hourly: 01 * * * *
    daily: 02 04 * * *
    weekly: 22 04 * * 0
    monthly: 42 05 01 * * 

