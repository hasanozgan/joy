# Joy Framework Default Routing Definitions

action:
    url: /(.+):controller/(.+):action 

default-action:
    url: /(.+):controller
    action: index

default-module:
    url: /
    controller: Dahius
    action: index

# ToDo Your Routing Settings

blog:
    url: /blog/(.+):action
    controller: Dahius_Blog

username:
    url: /(.+):username/
    controller: Dahius_Membership
    action: profile
